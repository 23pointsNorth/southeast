= content main
  .content-centered role="main"
    {{ $paginator := .Paginate .Data.Pages }}
    h1 Tag: {{ .Title }}
    p The content below has been tagged with the term &ldquo;{{ .Title }}.&rdquo;
    ul.teaser-list
    {{ range  $paginator.Pages }}
      {{ .Render "teaser" }}
    {{ end }}
    {{ partial "pagination.html" . }}
