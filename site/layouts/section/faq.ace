= content main
  .content-centered
    br
    h1.page-title Frequently asked questions

    {{ $section := lower .Title }}

    {{ $paginator := .Paginate (where .Data.Pages "Section" $section) 7 }}
    ul.card-list
      {{ range $paginator.Pages }}
        {{ .Render "summary" }}
      {{ end }}

    {{ partial "pagination.html" . }}

    script src="/js/infinite-scroll.js"
