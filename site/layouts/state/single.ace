= content main
  h1.page-title {{ .Title }}

  h2 Fast Facts
  ul.threatened-species.fade-list
  ul.endangered-species.fade-list
  ul.at-risk-species.fade-list

  {{ .Content }}

  ul.tag-list
    {{ range .Params.tags }}
    li.tag
      a href=/tags/{{ . | urlize }} {{ . }}
    {{ end }}

  span.state-name {{ .Title }}
  span.state-abbreviation {{ .Params.abbreviation }}
