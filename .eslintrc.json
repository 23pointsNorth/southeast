{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "radix": [2, "as-needed"],
    "comma-dangle": [2, "never"],
    "no-console": 0,
    "arrow-parens": [0, "as-needed"],
    "import/no-extraneous-dependencies": [
      2,
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ]
  }
}
