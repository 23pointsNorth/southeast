{
  "name": "southeast",
  "version": "1.0.0",
  "description": "Source code the the Southeast Region website of the U.S. Fish and Wildlife Service",
  "main": "index.js",
  "watch": {
    "svg": "src/images/svg/*.svg"
  },
  "scripts": {
    "test": "mocha",
    "npm-watch": "npm-watch",
    "hero": "node build/hero build",
    "images": "node build/images build",
    "watch": "node build/watch",
    "watch:sass": "node-sass -w src/scss/styles.scss dist/css/styles.css",
    "build:sass": "node-sass --output-style compressed src/scss/styles.scss | postcss -u autoprefixer -o dist/css/styles.css",
    "svg": "svg-sprite --si true --symbol-dest '' --svg-xmldecl false --ss sprite.svg -sD site/layouts/partials/ src/images/svg/*.svg",
    "mkdist": "node build/create-dist",
    "hugo:stage": "node build/stageHugo",
    "serve": "browser-sync start --server 'dist' --files 'dist/**/*' --log-level 'silent'",
    "prestart": "npm run mkdist",
    "start": "npm run npm-watch & npm run watch & npm run watch:sass & npm run serve",
    "build": "node build/build",
    "predeploy": "npm run mkdist && npm run build:sass",
    "deploy": "node build/build https://www.fws.gov/southeast/",
    "prestage": "npm run mkdist && npm run build:sass",
    "stage": "node build/build https://intranet.fws.gov/region4/demo/"
  },
  "keywords": [
    "Government",
    "USFWS"
  ],
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD",
  "devDependencies": {
    "async": "^2.1.5",
    "autoprefixer": "^6.7.7",
    "browser-sync": "^2.18.8",
    "chalk": "^1.1.3",
    "chokidar": "^1.4.3",
    "concat-map": "0.0.1",
    "csvtojson": "^1.1.2",
    "glob": "^7.0.3",
    "gray-matter": "^2.1.1",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "jsonlint": "^1.6.2",
    "jsonminify": "^0.4.1",
    "make-runnable": "^1.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "moment": "^2.17.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.4.0",
    "npm-watch": "^0.1.7",
    "postcss-cli": "^2.5.1",
    "replace-in-file": "^2.5.0",
    "rimraf": "^2.6.0",
    "sharp": "^0.17.2",
    "sitespeed.io": "^4.5.1",
    "svg-sprite": "^1.3.1",
    "svg-sprite-generator": "0.0.7",
    "titlecase": "^1.1.2",
    "yamljs": "^0.2.6"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/USFWS/southeast.git"
  },
  "bugs": {
    "url": "https://github.com/USFWS/southeast/issues"
  },
  "homepage": "https://github.com/USFWS/southeast#readme"
}
