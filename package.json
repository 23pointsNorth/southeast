{
  "name": "southeast",
  "version": "1.0.0",
  "description": "Source code the the Southeast Region website of the U.S. Fish and Wildlife Service",
  "main": "index.js",
  "watch": {
    "svg": "src/images/svg/*.svg"
  },
  "scripts": {
    "test": "mocha",
    "accessibility": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml --json pa11y.json > test/results/pa11y.json",
    "npm-watch": "npm-watch",
    "hero": "node build/hero build",
    "images": "node build/images build",
    "copy": "node build/images copy",
    "static": "ncp src/static dist",
    "watch": "node build/watch",
    "data": "node build/processData",
    "watch:sass": "node-sass -w src/scss/styles.scss dist/css/styles.css",
    "build:sass": "node-sass --output-style compressed src/scss/styles.scss | postcss -c build/postcss.config.js -o dist/css/styles.css",
    "svg": "svg-sprite --si true --symbol-dest '' --svg-xmldecl false --ss sprite.svg -sD site/layouts/partials/ src/images/svg/*.svg",
    "mkdist": "node build/create-dist",
    "hugo:stage": "node build/stageHugo",
    "prehugo:deploy": "npm run mkdist && npm run build:sass && npm run static",
    "hugo:deploy": "node build/deployHugo",
    "serve": "browser-sync start --server 'dist' --files 'dist/**/*' --log-level 'silent'",
    "prestart": "npm run mkdist && npm run build:sass && npm run static",
    "start": "npm run npm-watch & npm run watch & npm run watch:sass & npm run serve",
    "build": "node build/build",
    "predeploy": "npm run mkdist && npm run build:sass && npm run static",
    "deploy": "node build/build https://www.fws.gov/southeast/",
    "prestage": "npm run mkdist && npm run build:sass && npm run static",
    "stage": "node build/build https://intranet.fws.gov/region4/demo/"
  },
  "keywords": [
    "Government",
    "USFWS"
  ],
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD",
  "devDependencies": {
    "async": "^2.5.0",
    "autoprefixer": "^7.1.2",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "concat-map": "0.0.1",
    "cssnano": "^3.10.0",
    "csvtojson": "^1.1.6",
    "date-and-time": "^0.5.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "glob": "^7.1.2",
    "gray-matter": "^3.0.3",
    "imagemin": "^5.3.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.1",
    "jsonlint": "^1.6.2",
    "jsonminify": "^0.4.1",
    "make-runnable": "^1.3.6",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "moment": "^2.17.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "npm-watch": "^0.1.9",
    "pa11y": "^4.11.0",
    "pa11y-ci": "^1.2.0",
    "postcss": "^6.0.8",
    "postcss-cli": "^4.1.0",
    "recursive-readdir": "^2.2.1",
    "replace-in-file": "^2.5.3",
    "rimraf": "^2.6.0",
    "sharp": "^0.18.2",
    "string": "^3.3.3",
    "svg-sprite": "^1.3.7",
    "svg-sprite-generator": "0.0.7",
    "titlecase": "^1.1.2",
    "uglifyjs": "^2.4.11",
    "vueify": "^9.4.1",
    "xml-parser": "^1.2.1",
    "yamljs": "^0.2.10"
  },
  "dependencies": {
    "replace-in-file": "^2.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/USFWS/southeast.git"
  },
  "bugs": {
    "url": "https://github.com/USFWS/southeast/issues"
  },
  "homepage": "https://github.com/USFWS/southeast#readme"
}
