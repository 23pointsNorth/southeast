{
  "name": "southeast",
  "version": "1.0.0",
  "description": "Source code the the Southeast Region website of the U.S. Fish and Wildlife Service",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "accessibility": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml --json pa11y.json > ./test/results/pa11y.json",
    "hero": "node build/hero build",
    "images": "node build/images build",
    "copy": "node build/images copy",
    "static": "ncp src/static dist",
    "watch": "node build/watch",
    "data": "node build/processData && npm run hugo:deploy",
    "watch:sass": "node-sass -w src/scss/styles.scss site/static/css/styles.css",
    "build:sass": "node-sass --output-style compressed src/scss/styles.scss | postcss -c build/postcss.config.js -o site/static/css/styles.css",
    "mkdist": "node build/create-dist",
    "hugo:stage": "node build/stageHugo",
    "prehugo:deploy": "npm run mkdist && npm run build:sass",
    "hugo:deploy": "node build/deployHugo",
    "serve": "hugo server --config site/config.dev.yml -p 3000 -s site --navigateToChanged",
    "prestart": "npm run mkdist && npm run build:sass && npm run static",
    "start": "npm run watch & npm run watch:sass & npm run serve",
    "build": "node build/build",
    "predeploy": "npm run mkdist && npm run build:sass",
    "deploy": "node build/build https://www.fws.gov/southeast/",
    "prestage": "npm run mkdist && npm run build:sass && npm run static",
    "stage": "node build/build https://intranet.fws.gov/region4/demo/"
  },
  "keywords": [
    "Government",
    "USFWS"
  ],
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD",
  "devDependencies": {
    "async": "^2.5.0",
    "autoprefixer": "^7.2.6",
    "axe-cli": "^1.1.1",
    "chai": "^4.0.2",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "concat-map": "0.0.1",
    "cssnano": "^3.10.0",
    "csvtojson": "^1.1.6",
    "date-and-time": "^0.5.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "glob": "^7.1.2",
    "gray-matter": "^3.1.1",
    "imagemin": "^5.3.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.1",
    "jsonlint": "^1.6.2",
    "jsonminify": "^0.4.1",
    "make-runnable": "^1.3.6",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "moment": "^2.22.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.7.1",
    "pa11y": "^4.11.0",
    "pa11y-ci": "^1.2.0",
    "postcss": "^6.0.21",
    "postcss-cli": "^4.1.1",
    "recursive-readdir": "^2.2.1",
    "replace-in-file": "^3.1.0",
    "rimraf": "^2.6.2",
    "sharp": "^0.19.0",
    "string": "^3.3.3",
    "titlecase": "^1.1.2",
    "uglifyjs": "^2.4.11",
    "xml-parser": "^1.2.1",
    "yamljs": "^0.2.10"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/USFWS/southeast.git"
  },
  "bugs": {
    "url": "https://github.com/USFWS/southeast/issues"
  },
  "homepage": "https://github.com/USFWS/southeast#readme"
}
