{
  "name": "southeast",
  "version": "1.0.0",
  "description": "Source code the the Southeast Region website of the U.S. Fish and Wildlife Service",
  "main": "index.js",
  "watch": {
    "hugo": "site/**/*.{md,html,js,css}"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "mkdist": "node build/create-dist",
    "hugo:dev": "node build/hugo -- /",
    "hugo:stage": "node build/hugo -- https://intranet.fws.gov/region4/demo/",
    "hugo:pro": "node build/hugo -- http://www.fws.gov/southeast/",
    "serve": "browser-sync start --server --files 'dist/*'",
    "prestart": "npm run mkdist",
    "start": "npm run serve"
  },
  "keywords": [
    "Government",
    "USFWS"
  ],
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "browser-sync": "^2.11.1",
    "postcss-cli": "^2.5.1"
  }
}
