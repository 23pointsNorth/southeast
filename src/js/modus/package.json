{
  "name": "modus",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "csv2geojson": "npm run towers & npm run detections",
    "towers": "csv2geojson modus-towers.csv > ../../../dist/data/modus-towers.js",
    "detections": "csv2geojson modus-detections.csv > ../../../dist/data/modus-detections.js",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../dist/js/modus.js",
    "watchify": "watchify index.js -o ../../../dist/js/modus.js -dv",
    "start": "npm run csv2geojson && npm run watchify",
    "build": "npm run csv2geojson && npm run browserify"
  },
  "keywords": [],
  "author": "@r0yhewitt <roy_hewitt@fws.gov>",
  "license": "SAX-PD",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "csv2geojson": "^5.0.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "async": "^2.4.0",
    "leaflet": "^1.0.3",
    "leaflet.defaultextent": "0.0.3",
    "leaflet.featuregroup.subgroup": "^1.0.2",
    "leaflet.markercluster": "^1.0.4",
    "xhr": "^2.4.0"
  }
}
