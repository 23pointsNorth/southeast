{
  "name": "modus",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "csv2geojson": "npm run towers & npm run detections",
    "towers": "csv2geojson modus-towers.csv > ../../../site/static/data/modus-towers.js",
    "detections": "csv2geojson modus-detections.csv > ../../../site/static/data/modus-detections.js",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../site/static/js/modus.js",
    "watchify": "watchify index.js -o ../../../site/static/js/modus.js -dv",
    "start": "npm run csv2geojson && npm run watchify",
    "build": "npm run csv2geojson && npm run browserify"
  },
  "keywords": [],
  "author": "@r0yhewitt <roy_hewitt@fws.gov>",
  "license": "SAX-PD",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.4.0",
    "csv2geojson": "^5.1.1",
    "uglify-es": "^3.3.9",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "async": "^2.4.0",
    "leaflet": "^1.3.3",
    "leaflet.defaultextent": "0.0.3",
    "leaflet.featuregroup.subgroup": "^1.0.2",
    "leaflet.markercluster": "^1.0.5",
    "xhr": "^2.4.0"
  }
}
