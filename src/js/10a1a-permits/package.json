{
  "name": "10a1a-permits",
  "version": "1.0.0",
  "description": "A script that searches through JSON data of permits and returns the current permit status.",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "pugify"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../dist/js/10a1a-permits.js",
    "watchify": "watchify index.js -o ../../../dist/js/10a1a-permits.js -dv",
    "inspect": "browserify --full-paths index.js | uglifyjs -mc | Discify --open",
    "start": "npm run watchify",
    "build": "npm run browserify"
  },
  "keywords": [],
  "author": "Roy Hewitt <roy_hewitt@fws.gov>",
  "license": "ISC",
  "dependencies": {
    "xhr": "^2.3.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "disc": "^1.3.2",
    "pug": "^2.0.0-beta11",
    "pugify": "^2.1.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
