{
  "name": "every-page",
  "version": "1.0.0",
  "description": "Javascript used on every page",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "pugify"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../dist/js/every-page.js",
    "watchify": "watchify index.js -o ../../../dist/js/every-page.js -dv",
    "start": "npm run watchify",
    "build": "npm run browserify",
    "inspect": "browserify --full-paths index.js | uglifyjs -mc | Discify --open"
  },
  "author": "Roy Hewitt <royhewitt07@gmail.com>",
  "license": "SAX-PD",
  "dependencies": {
    "classlist-polyfill": "^1.0.3",
    "fws-glossary": "^1.2.0",
    "fws-navigation": "^1.6.0",
    "fws-scrollnav": "^1.7.0",
    "lodash.defaults": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "mark.js": "https://github.com/julmot/mark.js/archive/7.0.1.tar.gz",
    "parallax-scroll": "^0.3.0",
    "window.requestanimationframe": "^1.0.1",
    "xhr": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "disc": "^1.3.2",
    "pug": "^2.0.0-beta11",
    "pugify": "^2.1.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
