{
  "name": "jobs",
  "version": "1.0.0",
  "description": "Script to query the USA jobs API for USFWS listings in the Southeast",
  "main": "index.js",
  "browserify": {
    "transform": [
      "jadeify"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../dist/js/jobs.js",
    "watchify": "watchify index.js -o ../../../dist/js/jobs.js -dv",
    "inspect": "browserify --full-paths index.js | uglifyjs -mc | Discify --open",
    "start": "npm run watchify",
    "build": "npm run browserify"
  },
  "keywords": [
    "USAjobs"
  ],
  "author": "Roy Hewitt <royhewitt07@gmail.com>",
  "license": "SAX-PD",
  "dependencies": {
    "date-and-time": "^0.3.0",
    "query-string": "^4.1.0",
    "xhr": "^2.2.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "disc": "^1.3.2",
    "jade": "^1.11.0",
    "jadeify": "^4.6.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
