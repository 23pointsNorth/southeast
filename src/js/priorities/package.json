{
  "name": "priorities.js",
  "version": "1.0.0",
  "description": "Script for the priorities webpage",
  "main": "index.js",
  "browserify-shim": {
    ".src/js/imageComparison.js": "ImageComparison"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../dist/js/priorities.js",
    "watchify": "watchify index.js -o ../../../dist/js/priorities.js -dv",
    "inspect": "browserify --full-paths index.js | uglifyjs -mc | Discify --open",
    "start": "npm run watchify",
    "build": "npm run browserify"
  },
  "author": "Roy Hewitt, @r0yhewitt",
  "license": "SAX-PD",
  "dependencies": {
    "events": "^1.1.0",
    "fws-scrollnav": "^1.9.0",
    "image-comparison": "^1.1.1",
    "is-dom": "^1.0.5",
    "lodash.debounce": "^4.0.3",
    "lodash.defaults": "^4.0.1",
    "lodash.filter": "^4.2.1",
    "lodash.foreach": "^4.1.0",
    "lodash.isarray": "^4.0.0",
    "parallaxify": "^0.1.1",
    "scrollreveal": "^3.1.1",
    "underscore.string": "^3.3.4",
    "zenscroll": "^3.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "disc": "^1.3.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
