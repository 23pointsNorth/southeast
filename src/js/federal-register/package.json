{
  "name": "federal-register",
  "version": "1.0.0",
  "description": "Script to search the Federal Register for documents originating from the SE Region of USFWS",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "browserify": "browserify index.js | uglifyjs -mc > ../../../dist/js/federal-register.js",
    "watchify": "watchify index.js -o ../../../dist/js/federal-register.js -dv",
    "inspect": "browserify --full-paths index.js | uglifyjs -mc | Discify --open",
    "start": "npm run watchify",
    "build": "npm run browserify"
  },
  "keywords": [
    "Federal Register"
  ],
  "author": "Roy Hewitt <roy_hewitt@fws.gov>",
  "license": "SAX-PD",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "disc": "^1.3.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "date-and-time": "^0.3.0",
    "jsonp": "^0.2.0"
  }
}
